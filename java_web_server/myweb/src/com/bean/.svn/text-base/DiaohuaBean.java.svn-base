package Table_bean;

public class DiaohuaBean {

	//9个字段
	private int diaohuaID;
	private String diaohua_name;
	private float diaohua_price;
	private float suanliao;//算料
	private float kailiao;//开料
	private float mugong;//木工
	private float diqi;//底漆
	private float pendi;//喷底
	private float penmian;//喷面
	
	public DiaohuaBean()
	{
		super();
	}

	/**
	 * @return the diaohuaID
	 */
	public int getDiaohuaID() {
		return diaohuaID;
	}

	/**
	 * @param diaohuaID the diaohuaID to set
	 */
	public void setDiaohuaID(int diaohuaID) {
		this.diaohuaID = diaohuaID;
	}

	/**
	 * @return the diaohua_name
	 */
	public String getDiaohua_name() {
		return diaohua_name;
	}

	/**
	 * @param diaohua_name the diaohua_name to set
	 */
	public void setDiaohua_name(String diaohua_name) {
		this.diaohua_name = diaohua_name;
	}

	/**
	 * @return the diaohua_price
	 */
	public float getDiaohua_price() {
		return diaohua_price;
	}

	/**
	 * @param diaohua_price the diaohua_price to set
	 */
	public void setDiaohua_price(float diaohua_price) {
		this.diaohua_price = diaohua_price;
	}

	/**
	 * @return the suanliao
	 */
	public float getSuanliao() {
		return suanliao;
	}

	/**
	 * @param suanliao the suanliao to set
	 */
	public void setSuanliao(float suanliao) {
		this.suanliao = suanliao;
	}

	/**
	 * @return the kailiao
	 */
	public float getKailiao() {
		return kailiao;
	}

	/**
	 * @param kailiao the kailiao to set
	 */
	public void setKailiao(float kailiao) {
		this.kailiao = kailiao;
	}

	/**
	 * @return the mugong
	 */
	public float getMugong() {
		return mugong;
	}

	/**
	 * @param mugong the mugong to set
	 */
	public void setMugong(float mugong) {
		this.mugong = mugong;
	}

	/**
	 * @return the diqi
	 */
	public float getDiqi() {
		return diqi;
	}

	/**
	 * @param diqi the diqi to set
	 */
	public void setDiqi(float diqi) {
		this.diqi = diqi;
	}

	/**
	 * @return the pendi
	 */
	public float getPendi() {
		return pendi;
	}

	/**
	 * @param pendi the pendi to set
	 */
	public void setPendi(float pendi) {
		this.pendi = pendi;
	}

	/**
	 * @return the penmian
	 */
	public float getPenmian() {
		return penmian;
	}

	/**
	 * @param penmian the penmian to set
	 */
	public void setPenmian(float penmian) {
		this.penmian = penmian;
	}
	
}
