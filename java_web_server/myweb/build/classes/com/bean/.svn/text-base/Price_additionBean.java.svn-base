	public void setAddition_cost(float addition_cost) {
		this.addition_cost = addition_cost;
	}

	/**
	 * @return the addition_sign
	 */
	public int getAddition_sign() {
		return addition_sign;
	}

	/**
	 * @param addition_sign the addition_sign to set
	 */
	public void setAddition_sign(int addition_sign) {
		this.addition_sign = addition_sign;
	}

	/**
	 * @return the suanliao
