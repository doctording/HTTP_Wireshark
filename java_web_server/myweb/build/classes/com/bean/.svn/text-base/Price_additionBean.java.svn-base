	private float wujin;//hardware;
	
	public Price_additionBean()
	{
		super();
	}

	/**
	 * @return the additionID
	 */
	public int getAdditionID() {
		return additionID;
	}

	/**
	 * @param additionID the additionID to set
	 */
	public void setAdditionID(int additionID) {
		this.additionID = additionID;
	}

	/**
	 * @return the addition_name
	 */
	public String getAddition_name() {
		return addition_name;
	}

	/**
	 * @param addition_name the addition_name to set
	 */
	public void setAddition_name(String addition_name) {
		this.addition_name = addition_name;
	}
	public String getAddition_unit() {
		return addition_unit;
	}

	public void setAddition_unit(String addition_unit) {
		this.addition_unit = addition_unit;
	}
	/**
	 * @return the addition_price
	 */
	public float getAddition_price() {
		return addition_price;
	}

	/**
	 * @param addition_price the addition_price to set
	 */
	public void setAddition_price(float addition_price) {
		this.addition_price = addition_price;
	}

	/**
	 * @return the addition_cost
	 */
	public float getAddition_cost() {
		return addition_cost;
	}

