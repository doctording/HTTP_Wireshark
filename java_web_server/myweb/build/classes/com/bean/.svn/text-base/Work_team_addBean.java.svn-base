package Table_bean;

public class Work_team_addBean {

	private int id;
	private String work_team;//工作组
	private String group_members;//工作组成员
	private float gongling;//工龄
	private float daigong_injury;//代工工伤
	private float living_expenses;//生活费
	private float month_holidays;//月规定假期
	private float month_expenses;//月开支
	private float salary;
	private float ratio;
	private int sign;
	
	public float getSalary() {
		return salary;
	}

	public void setSalary(float salary) {
		this.salary = salary;
	}

	public float getRatio() {
		return ratio;
	}

	public void setRatio(float ratio) {
		this.ratio = ratio;
	}

	public int getSign() {
		return sign;
	}

	public void setSign(int sign) {
		this.sign = sign;
	}

	public Work_team_addBean(){
